<% @questions.each do |question| %>
  <h1>Question Title:<%= question.title %></h1>
  <h4>Question Object:<%= question %></h4>
  <h2>Question Content:<%= question.content %></h2>
  <br><br>
<% end %>

<%= form_for :question do |f| %>
  <%= f.label :"Question:" %>
  <%= f.text_field :title %>
  <%= f.label :"Content:" %>
  <%= f.text_area :content %>
  <%= f.submit %>
<% end %>
